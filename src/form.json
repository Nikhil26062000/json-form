[
  {
    "sectionTitle": "Preferences",
    "fields": [
      {
        "name": "experienceType",
        "label": "Experience Type",
        "type": "select",
        "options": [
          { "label": "Frontend", "value": "frontend" },
          { "label": "Backend", "value": "backend" },
          { "label": "Full Stack", "value": "fullstack" }
        ],
        "required": true,
        "styles": {
          "padding": "8px",
          "border": "1px solid #ccc",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px",
          "backgroundColor": "#f9f9f9"
        }
      },
      {
        "name": "frontendFramework",
        "label": "Frontend Framework",
        "type": "text",
        "placeholder": "Specify framework",
        "visibility": { "field": "experienceType", "value": "frontend" },
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "frontendTools",
        "label": "Frontend Tools",
        "type": "text",
        "placeholder": "Specify tools",
        "visibility": { "field": "experienceType", "value":"frontend" },
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "backendLanguage",
        "label": "Backend Language",
        "type": "text",
        "placeholder": "Specify language",
        "visibility": { "field": "experienceType", "value": "backend" },
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "database",
        "label": "Database",
        "type": "text",
        "placeholder": "Specify database",
        "visibility": { "field": "experienceType", "value": "backend" },
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "devOpsTools",
        "label": "DevOps Tools",
        "type": "text",
        "placeholder": "Specify tools",
        "visibility": { "field": "experienceType", "value": "backend" },
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "projectType",
        "label": "Project Type",
        "type": "text",
        "placeholder": "Describe your project",
        "visibility": { "field": "experienceType", "value": "fullstack" },
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "languages",
        "label": "Languages",
        "type": "checkbox",
        "options": [
          { "label": "JavaScript", "value": "javascript" },
          { "label": "Python", "value": "python" },
          { "label": "Java", "value": "java" }
        ],
        "required": false,
        "styles": {
          "display": "flex",
          "flexDirection": "column",
          "marginBottom": "10px"
        }
      },
      {
        "name": "preferredIDE",
        "label": "Preferred IDE",
        "type": "radio",
        "options": [
          { "label": "VSCode", "value": "vscode" },
          { "label": "IntelliJ", "value": "intellij" },
          { "label": "Eclipse", "value": "eclipse" }
        ],
        "required": true,
        "styles": {
          "display": "flex",
          "flexDirection": "row",
          "gap": "10px",
          "marginBottom": "10px"
        }
      },
      {
        "name": "projectDeadline",
        "label": "Project Deadline",
        "type": "date",
        "placeholder": "Select a date",
        "required": true,
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "projectName",
        "label": "Project Name",
        "type": "text",
        "placeholder": "Enter project name",
        "required": true,
        "events": {
          "onKeyUp": "handleProjectNameKeyUp",
          "onBlur": "handleProjectNameBlur",
          "onFocus": "handleProjectNameFocus"
        },
        "para":"Vij",

        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      },
      {
        "name": "projectPassword",
        "label": "Project Password",
        "type": "password",
        "placeholder": "Enter project password",
        "required": true,
        "events": {
          "onMouseOver": "handlePasswordMouseOver",
          "onMouseOut": "handlePasswordMouseOut"
        },
        "para":"Nikhil",
        "styles": {
          "padding": "8px",
          "border": "1px solid #ddd",
          "borderRadius": "4px",
          "width": "100%",
          "marginBottom": "10px"
        }
      }
    ]
  }
]
